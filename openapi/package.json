    "@types/node": "^16.11.12",
    "react": "^17.0.2",
    "react-redux": "^7.2.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.33"
  }
}
